<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE wuic SYSTEM 'http://capgemini.com/wuic/wuic.dtd'>
<wuic ehcache-provider="com.github.wuic.configuration.impl.DefaultEhCacheProvider">
    <resource-factory-builders>
        <resource-factory-builder id="appSourceRootProvider" class="com.github.wuic.servlet.WebappWuicResourceFactoryBuilder" />
    </resource-factory-builders>
    <groups>
        <group id="accordion-js" configuration="yuicompressor-js" default-builder="appSourceRootProvider">
            <file>jquery-ui-1.10.2/jquery-1.9.1.js</file>
            <file>jquery-ui-1.10.2/ui/jquery.ui.core.js</file>
            <file>jquery-ui-1.10.2/ui/jquery.ui.widget.js</file>
            <file>jquery-ui-1.10.2/ui/jquery.ui.accordion.js</file>
        </group>
        <group id="demo-css" configuration="yuicompressor-css" default-builder="appSourceRootProvider">
            <file>jquery-ui-1.10.2/themes/base/jquery.ui.all.css</file>
            <file>jquery-ui-1.10.2/demos/demos.css</file>
        </group>
    </groups>
    <configurations>
        <configuration id="yuicompressor-js" type="JAVASCRIPT">
            <cache>true</cache>
            <aggregate>true</aggregate>
            <charset>UTF-8</charset>
            <compress>true</compress>
            <yuiLineBreakPos>-1</yuiLineBreakPos>
            <yuiDisableOptimizations>false</yuiDisableOptimizations>
            <yuiMunge>true</yuiMunge>
            <yuiPreserveAllSemiColons>false</yuiPreserveAllSemiColons>
            <yuiVerbose>true</yuiVerbose>
        </configuration>
        <configuration id="yuicompressor-css" type="CSS">
            <cache>true</cache>
            <aggregate>true</aggregate>
            <charset>UTF-8</charset>
            <compress>true</compress>
            <yuiLineBreakPos>-1</yuiLineBreakPos>
        </configuration>
    </configurations>
</wuic>