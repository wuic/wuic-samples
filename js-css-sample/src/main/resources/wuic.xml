<?xml version="1.0" encoding="UTF-8"?>

<wuic>
    <nut-dao-builders>
        <nut-dao-builder id="appResourceFactoryBuilder" type="WebappNutDaoBuilder">
            <properties>
                <property key="c.g.wuic.dao.basePath">jquery-ui-1.10.2</property>
            </properties>
        </nut-dao-builder>
        <nut-dao-builder id="httpResourceFactoryBuilder" type="HttpNutDaoBuilder">
            <properties>
                <property key="c.g.wuic.dao.serverPort">8080</property>
                <property key="c.g.wuic.dao.basePath">jquery-ui-1.10.2</property>
            </properties>
        </nut-dao-builder>
    </nut-dao-builders>
    <engine-builders>
        <engine-builder id="wuicDefaultTextAggregatorEngineBuilder" type="TextAggregatorEngineBuilder">
            <properties>
                <property key="c.g.wuic.engine.aggregate">true</property>
            </properties>
        </engine-builder>

        <engine-builder id="wuicDefaultYuiCompressorCssEngineBuilder" type="YuiCompressorCssEngineBuilder">
            <properties>
                <property key="c.g.wuic.engine.compress">true</property>
            </properties>
        </engine-builder>
    </engine-builders>
    <heaps>
        <heap id="collapsible-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.accordion.js</nut-path>
        </heap>
        <heap id="custom-icons-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.accordion.js</nut-path>
            <nut-path>ui/jquery.ui.button.js</nut-path>
        </heap>
        <heap id="fillspace-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.accordion.js</nut-path>
            <nut-path>ui/jquery.ui.mouse.js</nut-path>
            <nut-path>ui/jquery.ui.resizable.js</nut-path>
        </heap>
        <heap id="sortable-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.accordion.js</nut-path>
            <nut-path>ui/jquery.ui.mouse.js</nut-path>
            <nut-path>ui/jquery.ui.sortable.js</nut-path>
        </heap>
        <heap id="effect-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.effect.js</nut-path>
        </heap>
        <heap id="autocomplete-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.position.js</nut-path>
            <nut-path>ui/jquery.ui.menu.js</nut-path>
            <nut-path>ui/jquery.ui.autocomplete.js</nut-path>
        </heap>
        <heap id="tooltip-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.button.js</nut-path>
            <nut-path>ui/jquery.ui.position.js</nut-path>
            <nut-path>ui/jquery.ui.menu.js</nut-path>
            <nut-path>ui/jquery.ui.autocomplete.js</nut-path>
            <nut-path>ui/jquery.ui.tooltip.js</nut-path>
        </heap>
        <heap id="button-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.button.js</nut-path>
        </heap>
        <heap id="splitbutton-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.button.js</nut-path>
            <nut-path>ui/jquery.ui.position.js</nut-path>
            <nut-path>ui/jquery.ui.menu.js</nut-path>
        </heap>
        <heap id="datepicker-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.datepicker.js</nut-path>
        </heap>
        <heap id="effect-datepicker-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.effect.js</nut-path>
            <nut-path>ui/jquery.ui.effect-blind.js</nut-path>
            <nut-path>ui/jquery.ui.effect-bounce.js</nut-path>
            <nut-path>ui/jquery.ui.effect-clip.js</nut-path>
            <nut-path>ui/jquery.ui.effect-drop.js</nut-path>
            <nut-path>ui/jquery.ui.effect-fold.js</nut-path>
            <nut-path>ui/jquery.ui.effect-slide.js</nut-path>
            <nut-path>ui/jquery.ui.datepicker.js</nut-path>
        </heap>
        <heap id="datepicker-loc-js" dao-builder-id="httpResourceFactoryBuilder">
            <nut-path>jquery-1.9.1.js</nut-path>
            <nut-path>ui/jquery.ui.core.js</nut-path>
            <nut-path>ui/jquery.ui.widget.js</nut-path>
            <nut-path>ui/jquery.ui.datepicker.js</nut-path>
            <nut-path>demos/datepicker/jquery.ui.datepicker-ar.js</nut-path>
            <nut-path>demos/datepicker/jquery.ui.datepicker-fr.js</nut-path>
            <nut-path>demos/datepicker/jquery.ui.datepicker-he.js</nut-path>
            <nut-path>demos/datepicker/jquery.ui.datepicker-zh-TW.js</nut-path>
        </heap>

        <heap id="demo-css" dao-builder-id="appResourceFactoryBuilder">
            <nut-path>themes/base/jquery.ui.all.css</nut-path>
            <nut-path>demos/demos.css</nut-path>
        </heap>
    </heaps>
    
    <workflows>
        <workflow id-prefix="" heap-id-pattern=".*-js">
            <without>
                <engine-builder-id>wuicDefaultMemoryMapCacheEngineBuilder</engine-builder-id>
            </without>
            <engine-chain>
                <engine-builder-id>wuicDefaultEhCacheEngineBuilder</engine-builder-id>
                <engine-builder-id>wuicDefaultYuiCompressorJavascriptEngineBuilder</engine-builder-id>
                <engine-builder-id>wuicDefaultTextAggregatorEngineBuilder</engine-builder-id>
            </engine-chain>
        </workflow>
        <workflow id-prefix="" heap-id-pattern=".*-css">
            <without>
                <engine-builder-id>wuicDefaultMemoryMapCacheEngineBuilder</engine-builder-id>
            </without>
            <engine-chain>
                <engine-builder-id>wuicDefaultEhCacheEngineBuilder</engine-builder-id>
                <engine-builder-id>wuicDefaultYuiCompressorCssEngineBuilder</engine-builder-id>
                <engine-builder-id>wuicDefaultTextAggregatorEngineBuilder</engine-builder-id>
            </engine-chain>
        </workflow>
    </workflows>
</wuic>