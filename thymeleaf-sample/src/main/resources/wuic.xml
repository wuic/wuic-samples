<?xml version="1.0" encoding="UTF-8"?>

<wuic>
    <nut-dao-builders>
        <nut-dao-builder id="nutDaoBuilder" type="WebappNutDaoBuilder">
            <properties>
                <property key="c.g.wuic.dao.regex">true</property>
            </properties>
        </nut-dao-builder>
    </nut-dao-builders>

    <heaps>
        <heap id="datatable-js" dao-builder-id="nutDaoBuilder">
            <nut-path>media/js/jquery.js</nut-path>
            <nut-path>media/js/jquery.dataTables.js</nut-path>
            <nut-path>sample/scroller/media/js/dataTables.scroller.js</nut-path>
        </heap>
        <heap id="datatable-css" dao-builder-id="nutDaoBuilder">
            <nut-path>.*.css</nut-path>
        </heap>
    </heaps>
</wuic>